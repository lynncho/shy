{
  "name": "lynncho/shy",
  "description": "This is a mini but powerful framework, concise and expansive, which can meet many needs.",
  "keywords": [
    "framework",
    "shy",
    "mvc"
  ],
  "license": "MIT",
  "type": "project",
  "homepage": "https://github.com/lynncho/shy",
  "require": {
    "php": "^7.0",
    "smarty/smarty": "^3.1",
    "workerman/workerman": "^3.5",
    "illuminate/database": "^5.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "app\\": "app/",
      "shy\\": "shy/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests/"
    }
  }
}
