#!/usr/bin/env php
<?php
/**
 * Shy Framework Console Entry
 *
 * @author    lynn<admin@lynncho.cn>
 * @link      http://lynncho.cn/
 */

use shy\console;

/**
 * Composer Autoload
 */
require __DIR__ . '/vendor/autoload.php';
/**
 * Bootstrap
 */
require __DIR__ . '/bootstrap/console.php';
/**
 * Run CLI mode
 */
config_set('IS_CLI', true);
shy(console::class)->runCommand();
